;;                                                                         ;;
;;                         Ontologia Literaria                             ;;
;;                                                                         ;;
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;; Llibres: 107                                                            ;;
;; - Atributs: escrit per, pertany a, any de publicació, edat mínima,      ;;
;;             editorial, número de pàgines, best seller,                  ;;
;;             adaptació a pel·lícula, saga, format, títol.                ;;
;;                                                                         ;;
;; Lectors: 52                                                             ;;
;; - Atributs: preferència per autor, preferència per gènere,              ;;
;;             llibres preferits, interessos acadèmics,                      ;;
;;             preferència per adaptacions a pel·lícules,                  ;;
;;             preferència per best sellers, preferència per sagas,        ;;
;;             preferència per tipus de lectura, ventall de pàgines,       ;;
;;             any de naixement, gènere.                                   ;;
;;                                                                         ;;
;; Autors: 97                                                              ;;
;; - Atributs: gènere predeterminat, tipus de lectura,                     ;;
;;             tipus de llenguatge, any de naixement, gènere,              ;;
;;             nom, idioma.                                                ;;
;;                                                                         ;;
;; Gèneres: 12                                                             ;;
;; - Atributs: nom del gènere, finalitat.                                  ;;
;;                                                                         ;;
;; Aquest document defineix una ontologia per un sistema recomanador de    ;;
;; llibres, incloent detalls sobre llibres, autors, gèneres i lectors.     ;;
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;


;;**************
;;* DEFCLASSES *
;;**************

(defclass Persona
    (is-a USER)
    (role concrete)
    (pattern-match reactive)
    (slot any_naixement
        (type INTEGER)
        (create-accessor read-write))
    (slot genere_persona
        (type STRING)
        (create-accessor read-write))
)

(defclass Autor_a
    (is-a Persona)
    (role concrete)
    (pattern-match reactive)
    (multislot genere_predeterminat
        (type INSTANCE)
        (create-accessor read-write))
    (slot tipus_lectura
        (type SYMBOL)
        (create-accessor read-write))
    (slot nom
        (type SYMBOL)
        (create-accessor read-write))
    (slot tipus_llenguatge
        (type STRING)
        (create-accessor read-write))
    (multislot idioma
        (type STRING)
        (create-accessor read-write))
)

(defclass Lector_a
    (is-a Persona)
    (role concrete)
    (pattern-match reactive)
    (multislot te_preferencia_per_autor
        (type INSTANCE)
        (create-accessor read-write))
    (multislot te_preferencia_per_genere
        (type INSTANCE)
        (create-accessor read-write))
    (multislot ha_llegit
        (type INSTANCE)
        (create-accessor read-write))
    (multislot interessos_academics
        (type STRING)
        (create-accessor read-write))
    (slot pref_adaptacio_peli
        (type STRING)
        (create-accessor read-write))
    (multislot pref_best_seller
        (type STRING)
        (create-accessor read-write))
    (multislot pref_tipus_lectura
        (type STRING)
        (create-accessor read-write))
    (multislot pref_sagues
        (type STRING)
        (create-accessor read-write))
    (slot ventall_pagines
        (type INTEGER)
        (create-accessor read-write))
)

(defclass Llibre
    (is-a USER)
    (role concrete)
    (pattern-match reactive)
    (slot escrit_per
        (type INSTANCE)
        (create-accessor read-write))
    (multislot pertany_a
        (type INSTANCE)
        (create-accessor read-write))
    (slot best_seller
        (type STRING)
        (create-accessor read-write))
    (slot adaptacio_a_pelicula
        (type STRING)
        (create-accessor read-write))
    (slot any_publicacio
        (type INTEGER)
        (create-accessor read-write))
    (slot edat_minima
        (type INTEGER)
        (create-accessor read-write))
    (multislot editorial
        (type STRING)
        (create-accessor read-write))
    (multislot format
        (type STRING)
        (create-accessor read-write))
    (slot num_pagines
        (type INTEGER)
        (create-accessor read-write))
    (slot saga
        (type STRING)
        (create-accessor read-write))
    (slot titol
        (type STRING)
        (create-accessor read-write))
    (multislot traduccions
        (type STRING)
        (create-accessor read-write))
    (slot valoracio
        (type STRING)
        (create-accessor read-write))
)

(defclass Genere
    (is-a USER)
    (role concrete)
    (pattern-match reactive)
    (multislot finalitat
        (type STRING)
        (create-accessor read-write))
    (slot genere
        (type STRING)
        (create-accessor read-write))
)


